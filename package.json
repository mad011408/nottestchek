    "posthog-node": "^5.17.2",
    "react": "^19.2.1",
    "react-dom": "19.2.1",
    "react-hotkeys-hook": "^5.2.1",
    "react-shiki": "^0.9.0",
    "react-textarea-autosize": "^8.5.9",
    "redis": "^5.10.0",
    "resumable-stream": "^2.2.8",
    "shiki": "^3.19.0",
    "sonner": "^2.0.7",
    "streamdown": "^1.6.10",
    "stripe": "^20.0.0",
    "tailwind-merge": "^3.4.0",
    "use-stick-to-bottom": "^1.1.1",
    "uuid": "^13.0.0",
    "word-extractor": "^1.0.4",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^30.2.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jszip": "^3.4.1",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/word-extractor": "^1.0.6",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "pnpm": {
    "overrides": {
      "axios": "1.12.2",
      "glob@>=10.0.0 <10.5.0": "10.5.0",
      "glob@>=11.0.0 <11.1.0": "11.1.0",
      "js-yaml": "4.1.1",
      "mdast-util-to-hast": "13.2.1",
      "vite": "7.1.11"
    }
  }
}
