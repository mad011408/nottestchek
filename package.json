    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.7",
    "@vercel/functions": "^3.3.4",
    "@workos-inc/authkit-nextjs": "^2.12.0",
    "@workos-inc/node": "^7.77.0",
    "ai": "^5.0.108",
    "ai-elements": "^1.6.3",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.29.2",
    "date-fns": "^4.1.0",
    "e2b": "^2.8.4",
    "gpt-tokenizer": "^3.4.0",
    "isbinaryfile": "^6.0.0",
    "jszip": "^3.10.1",
    "langchain": "^1.1.5",
    "lucide-react": "^0.556.0",
    "mammoth": "^1.11.0",
    "marked": "^17.0.1",
    "motion": "^12.23.25",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
